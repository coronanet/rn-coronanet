    "@react-navigation/stack": "^5.2.3",
    "@ui-kitten/components": "^4.4.0-beta.1",
    "@ui-kitten/eva-icons": "^4.4.1",
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-qrcode-svg": "^6.0.3",
